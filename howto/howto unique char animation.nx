'R = ROM INDEX
'S = SOURCE CHARA INDEX
'L = CHARA COUNT
'D = DEST CHARA INDEX
SUB UNCOLD(R,S,L,D)
COPY ROM(R)+S*16,L*16 TO $8000+D*16
END SUB

TEXT 1,1,"THIS EXAMPLE SHOW"
TEXT 1,2,"HOW TO USE ONLY"
TEXT 1,3,"ONE CHARA AND"
TEXT 1,4,"ANIMATE IT BY"
TEXT 1,5,"COPYING DATA FROM"
TEXT 1,6,"A ROM TO $8000."
TEXT 1,8,"YOU CAN ONLY HAVE"
TEXT 1,9,"256 CHARACTERS"
TEXT 1,10,"USED PER FRAME"
TEXT 1,11,"FOR SPRITES AND"
TEXT 1,12,"BACKGROUNDS."

SPRITE 0,134,26,1
A=0

DO
CALL UNCOLD(4,A,1,1)
A=(A+1) MOD 20
WAIT VBL
LOOP

