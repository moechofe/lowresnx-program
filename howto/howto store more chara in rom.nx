
'R = ROM INDEX
'S = SOURCE CHARA INDEX
'L = CHARA COUNT
'D = DEST CHARA INDEX
SUB UNCOLD(R,S,L,D)
COPY ROM(R)+S*16,L*16 TO $8000+D*16
END SUB

CALL UNCOLD(2,256,1,1)

TEXT 1,1,"THE MEMORY ALLOW"
TEXT 1,2,"TO STORE ONLY 256"
TEXT 1,3,"CHARACTERS, BUT"
TEXT 1,4,"THE ROM/FILE CAN"
TEXT 1,5,"STORE MORE THAN"
TEXT 1,6,"THAT."
TEXT 1,7,"IF YOU OPEN THIS"
TEXT 1,8,"CART IN THE"
TEXT 1,9,"CHARACTERS EDITOR"
TEXT 1,10,"YOU'LL WON'T BE"
TEXT 1,11,"ABLE TO SEE THE"
TEXT 1,12,"FACE SPRITE. IF"
TEXT 1,13,"YOU SAVE, YOU'LL"
TEXT 1,14,"REMOVE IT."

DO
SPRITE 0,140+(TIMER\10) MOD 8,116,1
WAIT VBL
LOOP

