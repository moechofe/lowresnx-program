FILES
LOAD 1,$FF00
LOAD 5,$8000
LOAD 8,$A000
BG SOURCE $A000
BG 0
BG COPY 0,16,20,16 TO 0,0
BG 1
BG COPY 0,0,20,16 TO 0,0
PALETTE 1,0,16*3,0,0
S=0
T=1
I=6
DO
IF T>0 THEN
DEC T
ELSE
T=1
FOR J=0 TO 63
IF SPRITE.C(J)<207 THEN
SPRITE J,,,SPRITE.C(J)+1
ELSE
SPRITE OFF J
END IF
NEXT J
I=(I+1) MOD 40
FOR X=I TO 0 STEP -1
Y=I-X
TRACE X,Y
IF X<20 AND Y<16 AND CELL.C(X,Y)\32=1 THEN
SPRITE S,X*8,Y*8,193
SPRITE.A S,(1,,,1,)
S=(S+1) MOD 64
END IF
NEXT X
END IF
WAIT VBL
LOOP


